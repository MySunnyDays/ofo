<!--pages/scanCode/scanCode.wxml-->
<view class='container'>
  <view class='password-title'>
  <text>开锁密码</text>
  </view>
  <view class='password-content'>
    {{password}}
  </view>
  <view class='tips'>
    <text>请使用密码解锁，{{time}}S后开始计费</text>
    <view class='tips-action'>车辆有问题？<text class='tips-href' bindtap="bindtap"> 回首页去车辆报账</text>
    </view>
  </view>
</view>
